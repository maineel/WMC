<body id="gradient" style="height:870px; background-image:linear-gradient(white,rgb(0, 98, 234));">
    <main class="form-signin w-100 m-auto">
        <form class="form-content" action="/add_places" method="POST">
            <!-- <img class="mb-4" src="public\images\apple-touch-icon.png" alt="" width="72" height="57" > -->
            <h1 class="h3 mb-3 fw-normal" style="color: rgb(51, 53, 55);">Please Sign Up</h1>
            <div class="form-floating">
                <input type="text" name="img_name" class="form-control" id="floatingFName" placeholder="name@example.com">
                <label for="floatingFName">Place image Address</label>
            </div><div class="form-floating">
                <input type="text" name="heading" class="form-control" id="floatingFName" placeholder="name@example.com">
                <label for="floatingFName">Place name</label>
            </div><div class="form-floating">
                <input type="text" name="description" class="form-control" id="floatingFname" placeholder="name@example.com">
                <label for="floatingFName">Image description</label>
            </div>
            <div class="form-floating">
                <input type="text" name="card_title" class="form-control" id="floatingFName" placeholder="name@example.com">
                <label for="floatingFName">Card Title</label>
            </div>
            <div class="form-floating">
                <input type="text" name="img_class" class="form-control" id="floatingFName"
                    placeholder="Password">
                <label for="floatingFName">Image Class</label>
            </div>
            <div class="form-floating">
                <input type="text" name="explore_path" class="form-control" id="floatingFName" placeholder="/place_name">
