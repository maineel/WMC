<body class="body">
  <header class="p-3">
    <div class="container">
      <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
        <nav>
          <a target="_blank" style="text-decoration:none" class="logo-text" href="https://www.wizardingworld.com/">
            <img height="50px" width="50px" src="public/images/Hogwarts.png" style="padding-bottom: 15%;" alt="Hogwarts"></a>
        </nav>
      </div>
    </div>
    <div class="container">
      <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">

        <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
          <li><a href="landing_index.html" class="nav-link px-2 text-white">Home</a></li>
          <li><a href="#" class="nav-link px-2 text-white">Features</a></li>
          <li><a href="#" class="nav-link px-2 text-white">Pricing</a></li>
          <li><a href="#" class="nav-link px-2 text-white">FAQs</a></li>
          <li><a href="#" class="nav-link px-2 text-white">About</a></li>
        </ul>

        <form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3" role="search" method="post" action="/places">
          <input type="search" class="form-control form-control-dark" placeholder="Search..." aria-label="Search" name="search_btn">
        </form>
      </div>
    </div>
  </header>
      <% places.forEach(function(element) { %>
        <div class="places-card">
            <img src="<%= element.img_name %>" height="340 px" width="400px" class="<%= element.img_class %>" alt="Shell Cottage">
            <div class="card-body">
              <h5 class="<%= element.card_title %>"><U><%= element.heading %></U></h5>
              <%= element.description %><br>
              <a href="#" style="margin-top:20px;" class="btn btn-primary">Explore ➡️</a>
            </div>
          </div>
      <% }); %>
  <footer class="footer">
